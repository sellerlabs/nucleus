{
  "name": "sellerlabs/nucleus",
  "description": "Some core PHP utilities",
  "license": "MIT",
  "authors": [
    {
      "name": "Eduardo Trujillo",
      "email": "ed@sellerlabs.com"
    }
  ],
  "require-dev": {
    "phpunit/phpunit": "*",
    "mockery/mockery": "0.9.*",
    "phpbench/phpbench": "@dev"
  },
  "autoload": {
    "psr-4": {
      "SellerLabs\\Nucleus\\": "src/SellerLabs/Nucleus"
    },
    "files": [
      "src/SellerLabs/Nucleus/Support/intl.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\SellerLabs\\Nucleus\\": "tests/SellerLabs/Nucleus",
      "Benchmarks\\SellerLabs\\Nucleus\\": "benchmarks/SellerLabs/Nucleus",
      "Benchmarks\\SellerLabs\\Sanity\\": "benchmarks/SellerLabs/Sanity"
    }
  },
  "require": {
    "lstrojny/phpunit-function-mocker": "0.2.*"
  }
}
